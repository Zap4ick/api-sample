<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sanity Suite" parallel="methods" thread-count="3">
    <test name="Critical Path Tests">
        <groups>
            <run>
                <include name="sanity"/>
            </run>
        </groups>
        <packages>
            <package name="api.*"/>
        </packages>
    </test>
</suite>
